generator client {
  provider      = "prisma-client-js"
  output        = "./generated/client"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["auth", "public"]
}

model User {
  id             String           @id @db.Uuid
  email          String           @unique
  name           String
  avatarUrl      String?          @map("avatar_url")
  role           String           @default("client")
  createdAt      DateTime?        @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt      DateTime?        @default(now()) @map("updated_at") @db.Timestamptz(6)
  cart_items     cart_items[]
  favorite_items favorite_items[]
  orders         orders[]
  user_addresses user_addresses[]

  @@map("users")
  @@schema("public")
}

model Category {
  id           String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name         String
  description  String?
  slug         String     @unique
  createdAt    DateTime?  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt    DateTime?  @default(now()) @map("updated_at") @db.Timestamptz(6)
  imageUrl     String?    @map("image_url")
  parentId     String?    @map("parent_id") @db.Uuid
  displayOrder Int?       @default(0) @map("display_order")
  isActive     Boolean?   @default(true) @map("is_active")
  parent       Category?  @relation("CategoryChildren", fields: [parentId], references: [id], onUpdate: NoAction)
  children     Category[] @relation("CategoryChildren")
  products     Product[]

  @@index([displayOrder], map: "idx_categories_display_order")
  @@index([isActive], map: "idx_categories_is_active")
  @@index([parentId], map: "idx_categories_parent_id")
  @@map("categories")
  @@schema("public")
}

model Product {
  id                   String                 @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                 String
  description          String
  urlSlug              String                 @unique @map("url_slug")
  sku                  String                 @unique
  basePrice            Int                    @map("base_price")
  salePrice            Int?                   @map("sale_price")
  stock                Int?                   @default(0)
  categoryId           String                 @map("category_id") @db.Uuid
  subcategory          String?
  brand                String
  isPublished          Boolean?               @default(false) @map("is_published")
  images               Json
  createdAt            DateTime?              @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt            DateTime?              @default(now()) @map("updated_at") @db.Timestamptz(6)
  average_cost         Decimal?               @db.Decimal(10, 2)
  last_cost            Decimal?               @db.Decimal(10, 2)
  cart_items           cart_items[]
  favorite_items       favorite_items[]
  notifications        Notification[]
  order_items          order_items[]
  product_suppliers    product_suppliers[]
  product_variants     product_variants[]
  category             Category               @relation(fields: [categoryId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  purchase_order_items purchase_order_items[]
  stockMovements       StockMovement[]

  @@map("products")
  @@schema("public")
}

model order_items {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  order_id   String    @db.Uuid
  product_id String    @db.Uuid
  quantity   Int
  unit_price Int
  subtotal   Int
  createdAt  DateTime? @default(now()) @db.Timestamptz(6)
  orders     orders    @relation(fields: [order_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  products   Product   @relation(fields: [product_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([order_id], map: "idx_order_items_order_id")
  @@index([product_id], map: "idx_order_items_product_id")
  @@schema("public")
}

/// Este modelo contiene restricciones CHECK y/o RLS
model orders {
  id               String          @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  order_number     String          @unique
  user_id          String          @db.Uuid
  status           String          @default("PENDING")
  subtotal         Int
  shipping         Int?            @default(0)
  tax              Int?            @default(0)
  total            Int
  shipping_name    String
  shipping_email   String
  shipping_phone   String
  shipping_address String
  shipping_city    String
  shipping_region  String
  shipping_zip     String
  notes            String?
  tracking_number  String?
  payment_method   String?
  createdAt        DateTime?       @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt        DateTime?       @default(now()) @map("updated_at") @db.Timestamptz(6)
  amount           Decimal?        @db.Decimal
  user_address_id  String?         @db.Uuid
  payment_status   String?         @default("PENDING")
  paid_at          DateTime?       @db.Timestamptz(6)
  notifications    Notification[]
  order_items      order_items[]
  user_addresses   user_addresses? @relation(fields: [user_address_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users            User            @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  stock_movements  StockMovement[]

  @@index([createdAt(sort: Desc)], map: "idx_orders_created_at")
  @@index([order_number], map: "idx_orders_order_number")
  @@index([status], map: "idx_orders_status")
  @@index([user_id], map: "idx_orders_user_id")
  @@schema("public")
}

/// Este modelo contiene RLS
model product_variants {
  id               String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  product_id       String    @db.Uuid
  sku              String    @unique
  size             String?
  color            String?
  stock            Int?      @default(0)
  price_adjustment Int?      @default(0)
  is_active        Boolean?  @default(true)
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  updated_at       DateTime? @default(now()) @db.Timestamptz(6)
  products         Product   @relation(fields: [product_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([product_id, size, color], map: "unique_variant")
  @@index([product_id], map: "idx_product_variants_product_id")
  @@index([sku], map: "idx_product_variants_sku")
  @@schema("public")
}

model StockMovement {
  id             String            @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  productId      String            @map("product_id") @db.Uuid
  type           StockMovementType
  quantity       Int
  previousStock  Int               @map("previous_stock")
  newStock       Int               @map("new_stock")
  reason         String?
  userId         String?           @map("user_id") @db.Uuid
  createdAt      DateTime          @default(now()) @map("created_at") @db.Timestamptz(6)
  unit_cost      Decimal?          @db.Decimal
  unit_price     Decimal?          @db.Decimal
  total_value    Decimal?          @db.Decimal
  order_id       String?           @db.Uuid
  reference_type String?           @db.VarChar(50)
  reference_id   String?           @db.Uuid
  notes          String?
  orders         orders?           @relation(fields: [order_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  product        Product           @relation(fields: [productId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([productId], map: "idx_stock_movements_product_id")
  @@index([createdAt(sort: Desc)], map: "idx_stock_movements_created_at")
  @@index([order_id], map: "idx_stock_movements_order_id")
  @@index([reference_type, reference_id], map: "idx_stock_movements_reference")
  @@map("stock_movements")
  @@schema("public")
}

model Notification {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  type      String
  title     String
  message   String
  productId String?  @map("product_id") @db.Uuid
  orderId   String?  @map("order_id") @db.Uuid
  isRead    Boolean? @default(false) @map("is_read")
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamptz(6)
  order     orders?  @relation(fields: [orderId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  product   Product? @relation(fields: [productId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([createdAt(sort: Desc)], map: "idx_notifications_created_at")
  @@index([isRead], map: "idx_notifications_is_read")
  @@index([type], map: "idx_notifications_type")
  @@index([productId], map: "idx_notifications_product_id")
  @@map("notifications")
  @@schema("public")
}

model PageVisit {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  path       String
  timestamp  DateTime @default(now()) @db.Timestamptz(6)
  ip         String?
  user_agent String?

  @@index([timestamp], map: "idx_page_visits_timestamp")
  @@map("page_visits")
  @@schema("public")
}

